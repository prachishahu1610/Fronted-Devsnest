function Cylinder(height, radius){
    this.height = height;
    this.radius = radius;
    }
    Cylinder.prototype.volume = function(){
    return (Math.PI * Math.pow(this.radius, 2) * this.height).toFixed(4);
    }
    
    var myCylinder = new Cylinder(4 , 2);
    console.log(myCylinder.volume());
    
    var bigCylinder = new Cylinder( 850 , 10);
    console.log( "Volume of Big Cylinder : " + bigCylinder.volume());
